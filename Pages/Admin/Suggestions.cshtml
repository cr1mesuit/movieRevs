@page
@model MovieRevs.Pages.Admin.SuggestionsModel
@{
    ViewData["Title"] = "Предложения пользователей";
}

<h2>Предложения пользователей</h2>

<table>
    <tr>
        <th>Название</th>
        <th>Описание</th>
        <th>Предложил</th>
    </tr>
    @foreach (var s in Model.Suggestions)
    {
        <tr>
            <td>@s.Title</td>
            <td>@s.Description</td>
            <td>@s.SuggestedBy</td>
        </tr>
    }
</table>
